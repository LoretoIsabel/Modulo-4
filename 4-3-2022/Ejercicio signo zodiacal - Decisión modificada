import java.util.Scanner;

public class DecisionModificada {

	public static void main(String[] args) {
	int dia;
	int mes;
	int año;
	Scanner leer = new Scanner(System.in);
	
	System.out.println("Ingrese el día en que nació:  ");
	dia = leer.nextInt();
	
	System.out.println("Ingrese el mes en que nació:  ");
	mes = leer.nextInt();
	
	System.out.println("Ingrese el año en que nació:  ");
	año = leer.nextInt();
	
			
		if(mes == 1) {
			if(dia>=20)	{
		System.out.println("Usted es Acuario");
		} 
			else {
		System.out.println("Usted es Capricornio");
		}}
		
		else if(mes == 2) {
			if(dia>=20) {	
		System.out.println("Usted es Piscis");}
			else {
		System.out.println("Usted es Acuario");
		}}
		
		else if(mes == 3){	
			if (dia>=20) {
		System.out.println("Usted es Aries");}
			else{
		System.out.println("Usted es Piscis");}
		}

		else if(mes == 4) {
			if (dia>=20) {
		System.out.println("Usted es Aries");}
			else {
		System.out.println("Usted es Tauro");}
		}
		
		else if(mes == 5){	
			if (dia>=20) {
		System.out.println("Usted es Géminis");}
			else {
		System.out.println("Usted es Tauro");}
		}
		
		else if(mes == 6){
			if (dia>=20) {
		System.out.println("Usted es Cáncer");}
			else {
		System.out.println("Usted es Géminis");}
		}
		
		else if(mes == 7){
			if(dia>=20) {
		System.out.println("Usted es Leo");}
			else {
		System.out.println("Usted es Cáncer");}
			}
		
		else if(mes == 8){	
			if(dia >=20) {
		System.out.println("Usted es Virgo");}
			else {
		System.out.println("Usted es Leo");}
			}
		
		else if(mes == 9){	
			if (dia>=20) {
		System.out.println("Usted es Libra");}
			else {
		System.out.println("Usted es Virgo");}
			}
		
		else if(mes == 10 ){	
			if(dia>=20) {
		System.out.println("Usted es Escorpio");}
			else {
		System.out.println("Usted es Libra");}
			}
		
		else if(mes == 11){	
			if(dia>=20) {
		System.out.println("Usted es Sagitario");}
			else {
		System.out.println("Usted es Escorpio");}
			}

		else if(mes == 12){	
			if(dia>=20) {
		System.out.println("Usted es Capricornio");}
			else {
		System.out.println("Usted es Sagitario");}
			}
		
		else if (mes >=13)  {
			System.out.println("Ingresó mal los datos");}}

}
